<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <%- include('partials/head') %>

</head>
<body>
    <%- include('partials/logged_in_menu') %>
    <% for (let meal of rows) { %>
        <form action="/meals/edit">
            <input type="hidden" value="<%=meal.meal_Id%>" name="meal_Id">
            <button>Update</button>
        </form>
        <form action="/meals/delete" id="delete" method="post"> 
            <input type="hidden" value="<%=meal.meal_Id%>" name="meal_Id">
           
           <span id="meal"><%=meal.calories%></span> <br>
           <span id="meal"><%=meal.meal%></span> <br>
           <span id="meal"><%=meal.time%></span> <br>
           <span id="meal"><%=meal.date%></span> <br>
            <button type="submit" class="btn btn btn-dark">Delete</button>
        </form> <br> <br>
     <% } %>
</body>
</html>